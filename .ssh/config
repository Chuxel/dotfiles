# Read more about SSH config files: https://linux.die.net/man/5/ssh_config
Host devcontainer-root
	User root
	Hostname localhost
	Port 2222
	NoHostAuthenticationForLocalhost yes
	UserKnownHostsFile /dev/null

Host devcontainer-codespace
	User codespace
	Hostname localhost
	Port 2222
	NoHostAuthenticationForLocalhost yes
	UserKnownHostsFile /dev/null

Host devcontainer-vscode
	User vscode
	Hostname localhost
	Port 2222
	NoHostAuthenticationForLocalhost yes
	UserKnownHostsFile /dev/null

Host devcontainer-node
	User node
	Hostname localhost
	Port 2222
	NoHostAuthenticationForLocalhost yes
	UserKnownHostsFile /dev/null
